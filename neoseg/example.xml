<?xml version="1.0"?>
<!DOCTYPE SEGMENTATION-PARAMETERS>
<SEGMENTATION-PARAMETERS>
<SUFFIX>0123</SUFFIX>
<ATLAS-DIRECTORY>/my/atlas/path</ATLAS-DIRECTORY>
<ATLAS-ORIENTATION>RAI</ATLAS-ORIENTATION>
<ATLAS-FORMAT>nrrd</ATLAS-FORMAT>
<OUTPUT-DIRECTORY>/my/output/directory/path</OUTPUT-DIRECTORY>
<OUTPUT-FORMAT>GIPL</OUTPUT-FORMAT>
<IMAGE>
  <FILE>/my/input/image/path/T1-RAI.gipl</FILE>
  <ORIENTATION>RAI</ORIENTATION>
</IMAGE>
<IMAGE>
  <FILE>/my/input/image/path/T2-RAI.gipl</FILE>
  <ORIENTATION>RAI</ORIENTATION>
</IMAGE>
<FILTER-ITERATIONS>5</FILTER-ITERATIONS>
<FILTER-TIME-STEP>0.01</FILTER-TIME-STEP>
<FILTER-METHOD>Curvature flow</FILTER-METHOD>
<MAX-BIAS-DEGREE>4</MAX-BIAS-DEGREE>
<PRIOR-1>0.4</PRIOR-1>
<PRIOR-2>1.4</PRIOR-2>
<PRIOR-3>1</PRIOR-3>
<PRIOR-4>1</PRIOR-4>
<PRIOR-5>1</PRIOR-5>
<DO-ATLAS-WARP>1</DO-ATLAS-WARP>
<ATLAS-WARP-GRID-X>5</ATLAS-WARP-GRID-X>
<ATLAS-WARP-GRID-Y>5</ATLAS-WARP-GRID-Y>
<ATLAS-WARP-GRID-Z>5</ATLAS-WARP-GRID-Z>
<MAHALANOBIS-THRESHOLD>3</MAHALANOBIS-THRESHOLD>
<PARZEN-KERNEL-WIDTH>0.05</PARZEN-KERNEL-WIDTH>
<PRIOR-THRESHOLD>0.9</PRIOR-THRESHOLD>
<REFERENCE-IMAGE-INDEX>2</REFERENCE-IMAGE-INDEX>
<REFERENCE-MODALITY>T2</REFERENCE-MODALITY>
</SEGMENTATION-PARAMETERS>
